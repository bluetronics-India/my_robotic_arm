        <origin xyz="0 0 0" rpy="0 0 0"/>
        <parent link="joint5"/>
        <child link="link5"/>
    </joint>
    <link name="link5">
        <collision>
            <origin xyz="0 ${l5/2} 0" rpy="0 0 0"/>
            <geometry>
                <box size="${width} ${l5} ${width}" />
            </geometry>
        </collision>
        <visual>
            <origin xyz="0 ${l5/2} 0" rpy="0 0 0"/>
            <geometry>
                <box size="${width} ${l5}  ${width}"/>
            </geometry>
            <material name="green"/>
        </visual>
        <inertial>
            <origin xyz="0.0 0.00000 0.0" rpy="0 0 0"/>
            <mass value="0.5"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
        </inertial>
    </link>
