<?xml version="1.0"?>
<!-- Revolute-Revolute Manipulator -->
<robot name="my_robotic_arm" xmlns:xacro="http://www.ros.org/wiki/xacro">
    <material name="black">
        <color rgba="0.0 0.0 0.0 1.0"/>
    </material>
    
    <material name="blue">
        <color rgba="0.0 0.0 0.8 1.0"/>
    </material>
    
    <material name="green">
        <color rgba="0.0 0.8 0.0 1.0"/>
    </material>
    <material name="darkgreen">
        <color rgba="0.0 0.3 0.0 1.0"/>
    </material>
    
    <material name="gray">
        <color rgba="0.7 0.7 0.7 1.0"/>
    </material>
    
    <material name="darkgray">
        <color rgba="0.3 0.3 0.3 1.0"/>
    </material>
    
    <material name="red">
        <color rgba="0.8 0.0 0.0 1.0"/>
    </material>
    
    <material name="white">
        <color rgba="1.0 1.0 1.0 1.0"/>
    </material>
    
    <material name="yellow">
        <color rgba="0.8 0.8 0.0 1.0"/>
    </material>
    <!-- Constants -->
    <xacro:property name="deg_to_rad" value="0.01745329251994329577"/>
    <!--     <xacro:property name="pi" value="3.141592654" /> -->
    <xacro:property name="M_PI" value="3.141592654"/>
    
    <xacro:property name="l4" value="0.05"/>
    
    
    <xacro:property name="width" value="0.05"/>
    <xacro:property name="joint_radius" value="0.05"/>
    <xacro:property name="joint_height" value="0.2"/>
    
    
    <xacro:property name="l1" value="0.3"/>
    <xacro:property name="l2" value="0.3"/>
    <xacro:property name="l3" value="0.3"/>
    <xacro:property name="l4" value="0.2"/>
    <xacro:property name="l5" value="0.2"/>
    <xacro:property name="l6" value="0.3"/>
           
    <xacro:property name="end_ball_radius" value="0.05"/>
    
    <!-- Used for fixing robot to Gazebo 'link 1' -->
    <link name="world"/>
    <joint name="j1" type="revolute">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <parent link="world"/>
        <child link="joint1"/>
        <axis xyz="0 0 1"/>  
        <limit velocity="5.0" effort="100" lower="${-pi*0.75}" upper="${pi*0.75}"/>
        <dynamics damping="0.7"/>
    </joint>
    <link name="joint1">
        <collision>
            <origin xyz="0 0 ${0/2}" rpy="0 0 0"/>
            <geometry>
                <cylinder length="${joint_height}" radius="${joint_radius}" />
            </geometry>
        </collision>
        <visual>
            <origin xyz="0 0 ${0/2}" rpy="0 0 0"/>
            <geometry>
                <cylinder length="${joint_height}" radius="${joint_radius}" />
            </geometry>
        </visual>
        <inertial>
            <origin xyz="0.0 0.00000 0.0" rpy="0 0 0"/>
            <mass value="0.5"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
        </inertial>
    </link>
<!--    <joint name="link1_fix" type="fixed">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <parent link="joint1"/>
        <child link="link1"/>
    </joint>
    <link name="link1">
        <collision>
            <origin xyz="0 0 ${l1/2}" rpy="0 0 0"/>
            <geometry>
                <box size="${width} ${width} ${l1}" />
            </geometry>
        </collision>
        <visual>
            <origin xyz="0 0 ${l1/2}" rpy="0 0 0"/>
            <geometry>
                <box size="${width} ${width} ${l1}"/>
            </geometry>
            <material name="darkgreen"/>
        </visual>
        <inertial>
            <origin xyz="0.0 0.00000 0.0" rpy="0 0 0"/>
            <mass value="0.5"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
        </inertial>
    </link>
   --> 
    <joint name="j2" type="revolute">
        <origin xyz="0 0 ${l1}" rpy="${pi*0.5} ${pi*0.0} ${pi*0.5}"/>
        <parent link="joint1"/>
        <child link="joint2"/>
        <axis xyz="0 0 1 "/>  
        <limit velocity="5.0" effort="100" lower="${-pi*0.75}" upper="${pi*0.75}"/>
        <dynamics damping="0.7"/>
    </joint>
    <link name="joint2">
        <collision>
            <origin xyz=" 0 0 ${0/2}" rpy="0 ${-pi*0} 0"/>
            <geometry>
                <cylinder length="${joint_height}" radius="${joint_radius}" />
            </geometry>
        </collision>
        <visual>
            <origin xyz="0 0 ${0/2} " rpy="0 ${-pi*0} 0"/>
            <geometry>
                <cylinder length="${joint_height}" radius="${joint_radius}" />
            </geometry>
        </visual>
        <inertial>
            <origin xyz="0.0 0.00000 0.0" rpy="0 0 0"/>
            <mass value="0.5"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
        </inertial>
    </link>
<!--    <joint name="link2_fix" type="fixed">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <parent link="joint2"/>
        <child link="link2"/>
    </joint>
    <link name="link2">
        <collision>
            <origin xyz="0 ${l2/2} 0 " rpy="0 0 0"/>
            <geometry>
                <box size="${width} ${l2} ${width}" />
            </geometry>
        </collision>
        <visual>
            <origin xyz="0 ${l2/2} 0 " rpy="0 0 0"/>
            <geometry>
                <box size="${width} ${l2} ${width}"/>
            </geometry>
            <material name="darkgreen"/>
        </visual>
        <inertial>
            <origin xyz="0.0 0.00000 0.0" rpy="0 0 0"/>
            <mass value="0.5"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
        </inertial>
    </link>
    -->
    <joint name="j3" type="revolute">
        <origin xyz="0 ${l2} 0" rpy="${pi*0.0} ${pi*0.0} ${pi*0.5}"/> 
        <parent link="joint2"/>
        <child link="joint3"/>
        <axis xyz="0 0 1"/> 
        <limit velocity="5.0" effort="100" lower="${-pi*0.75}" upper="${pi*0.75}"/>
        <dynamics damping="0.7"/>
    </joint>
    <link name="joint3">
        <collision>
            <origin xyz="0 ${0/2} 0" rpy="0 ${-pi*0} 0"/>
            <geometry>
                <cylinder length="${joint_height}" radius="${joint_radius}" />
            </geometry>
        </collision>
        <visual>
            <origin xyz="0 ${0/2}  0" rpy="0 ${-pi*0} 0"/>
            <geometry>
                <cylinder length="${joint_height}" radius="${joint_radius}" />
            </geometry>
        </visual>
        <inertial>
            <origin xyz="0.0 0.00000 0.0" rpy="0 0 0"/>
            <mass value="0.5"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
        </inertial>
    </link>
<!--    <joint name="link3_fix" type="fixed">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <parent link="joint3"/>
        <child link="link3"/>
    </joint>
    <link name="link3">
        <collision>
            <origin xyz="${l3/2} 0 0 " rpy="0 0 0"/>
            <geometry>
                <box size="${l3} ${width} ${width}" />
            </geometry>
        </collision>
        <visual>
            <origin xyz="${l3/2} 0 0 " rpy="0 0 0"/>
            <geometry>
                <box size="${l3} ${width} ${width}"/>
            </geometry>
            <material name="darkgreen"/>
        </visual>
        <inertial>
            <origin xyz="0.0 0.00000 0.0" rpy="0 0 0"/>
            <mass value="0.5"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
        </inertial>
    </link>
    
    -->
    <joint name="j4" type="revolute">
        <origin xyz="${l3} 0 0" rpy="${pi*0.5} ${pi*0} ${pi*0.5}"/> 
        <parent link="joint3"/>
        <child link="joint4"/>
        <axis xyz="0 0 1"/> 
        <limit velocity="5.0" effort="100" lower="${-pi*0.75}" upper="${pi*0.75}"/>
        <dynamics damping="0.7"/>
    </joint>
    <link name="joint4">
        <collision>
            <origin xyz="0 ${0/2} 0" rpy="${pi*0.0} ${pi*0.0} ${pi*0.0}"/>
            <geometry>
                <cylinder length="${joint_height}" radius="${joint_radius}" />
